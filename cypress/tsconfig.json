{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strict": true,
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "cypress",
      "mock-socket",
      "node",
      "cypress-wait-until"
    ]
  },
  "include": [
    "**/*.ts",
    "../node_modules/cypress",
]
}